a
    font-family "Open Sans", sans-serif
    font-size 14px
body
    font-family "Open Sans", sans-serif
    font-size 14px
div.breadcrumbs
    float right
    height 30px
    width 92%
h3
    font-family "Open Sans", sans-serif
    margin-top 0px
    small
        padding-left 10px
li
    font-family "Open Sans", sans-serif
    font-size 14px
  	letter-spacing 1px

    &.active > a
        background-color #275CA8
        color #FFF
textarea
    width 100%
ul
    list-style none
    padding-left 20px
ul li
	list-style none
#content
    margin-top 70px
#content-main
    padding-top 20px
#header
    background-color #275CA8
    border-bottom 5px solid #FF5200
    height 60px
    img
        margin-top 10px
        position absolute !important
#modal-title-observacion
	strong
  		margin-left 20px
.border-lideres
    border 5px solid #FFF
    height 125px
    img
        height 110px
        width 90px
.btn-warning
    color #FFF !important
    background none !important
    background-color #F0AD4E !important
    border-color #EEA236 !important
.btn-danger
    color #FFF !important
    background none !important
    background-color #D9534F !important
    border-color #D43F3A !important
.content-breadcrumbs
    font-size 12px
    padding 3px 0px 3px 5%
.dashboard-stat
    margin-bottom 25px
    &.yellow
        background-color #FFDE8D
        .more
            background-color #ffd367
    &.blue
        background-color #7eadbd
        .more
            background-color #5492a7
    &.green
        background-color #a0d892
        .more
            background-color #80cc6e
    .visual
        display block
        float left
        font-size 35px
        height 50px
        line-height 35px
        margin-bottom 10px
        padding-left 15px
        padding-top 10px
        width 50px
        i
            font-size 45px
            line-height 45px
            color #FFF
    .details
        position absolute
        padding-right 10px
        right 15px
        .number
            color #FFF
            font-size 34px
            font-weight 300
            letter-spacing -1px
            line-height 34px
            margin-bottom 5px
            padding-top 15px
            text-align right
        .desc
            color #FFF
            font-size 16px
            font-weight 300
            letter-spacing 0px
            text-align right
    .more
        clear both
        display block
        padding 5px 10px
        text-transform uppercase
        font-weight 300
        font-size 11px
        color #FFF
        opacity 0.7
.foot-lideres
    font-family "Open Sans", sans-serif
    background-color #DFF0D8
    width 100%
    text-align center
    outline 1px solid #BFBFBF
.img-lideres
    display inline-block
    min-height 120px
    width 100px
    margin-left 10px
    outline 1px solid #BFBFBF
.marca_agua
    opacity 0.1
    position absolute
    right 30px
    top 35px
.note
    margin 0px 0px 5px
    padding 5px 7px 7px 15px
    border-left 5px solid #EEE
.note-success
    background-color #EBFCEE
    border-color #3CC051
.span-post-icon
    margin-left 20px
.user-pull
    color #275CA8
    i
        margin-left 7px
        margin-right 2px
    #user-name
        font-family "Open Sans", sans-serif
        margin-right 10px
.vertical-divider
    border-left 1px solid #F2F2F2
    border-right 1px solid #FFF